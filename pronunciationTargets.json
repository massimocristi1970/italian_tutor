  "pronunciation_tips": {
    "general": [
      "Italian has pure vowel sounds - avoid English diphthongs",
      "Every letter is pronounced - no silent letters (except 'h')",
      "Stress usually falls on the second-to-last syllable",
      "Double consonants are held longer - this is crucial!",
      "Roll your R's with the tip of your tongue"
    ],
    "vowels": [
      "A = 'ah' as in 'father'",
      "E = 'eh' as in 'bet' (open e) or 'ay' as in 'may' (closed e)",
      "I = 'ee' as in 'feet'",
      "O = 'oh' as in 'or' (open o) or 'oh' as in 'go' (closed o)",
      "U = 'oo' as in 'food'"
    ],
    "consonants": [
      "C before e/i = 'ch' sound (ciao, cena)",
      "C before a/o/u = 'k' sound (casa, come)",
      "G before e/i = 'j' sound (gelato, giro)",
      "G before a/o/u = hard 'g' (gatto, gusto)",
      "GN = 'ny' sound like Spanish √± (gnocchi)",
      "GLI = like 'lli' in million (figlio)",
      "SC before e/i = 'sh' sound (pesce)",
      "Z = 'ts' or 'dz' depending on word (grazie, zero)"
    ]
  }
